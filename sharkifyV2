    local l__ReplicatedStorage__1 = game:GetService("ReplicatedStorage");
local l__Modules__2 = l__ReplicatedStorage__1:WaitForChild("Modules");
local v10 = require(l__Modules__2:WaitForChild("Moneys"));

while wait() do
    v10:AddCash(200000);
    workspace:SetAttribute("Multiplier", workspace:GetAttribute("Multiplier") + 999999)
    workspace:SetAttribute("Paper", workspace:GetAttribute("Paper") + 999999)
    workspace:SetAttribute("Gold", workspace:GetAttribute("Gold") + 9999999)
end
end)





local Tab = Window:NewTab("skydive off a tower")

local Section = Tab:NewSection("main!")

Section:NewButton("inf cash kinda", "when you press this jump off.", function()
    --// DISCLAIMER: Don't use math.huge, that'll get you banned
--// USAGE: Just jump off your building and once you land, you'll receive the amout of cash set below
--

local A_Number = 50000000 --// This would be the cash you receive
local B_Number = 500 --// Idk wtf this shit is

local OldNamecall

OldNamecall = hookmetamethod(game, "__namecall", function(Self, ...)
    local Args = {...}

    if tostring(Self) == "validate_skydive" then
        Args[1] = A_Number
        Args[2] = B_Number

        return Self.FireServer(Self, unpack(Args))
    end

    return OldNamecall(Self, ...)
end)
end)


