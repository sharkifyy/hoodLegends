    while wait() do
        workspace:SetAttribute(Money, 2000000000)
        workspace:SetAttribute(Gold, 2000000000)
        workspace:SetAttribute(Multiplier, 200000000)
        workspace:SetAttribute(Paper, 2000000000)
    end -- safer way 
end)

